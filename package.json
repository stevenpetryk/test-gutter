{
  "name": "atom-spec-gutter",
  "main": "./lib/main",
  "version": "0.2.4",
  "description": "RSpec results, right in your gutter.",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": "atom-spec-gutter:toggle"
  },
  "scripts": {
    "test": "standard"
  },
  "repository": "https://github.com/stevenpetryk/atom-spec-gutter",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "concat-stream": "^1.5.1"
  },
  "devDependencies": {
    "standard": "*"
  },
  "activationHooks": [
    "language-rspec:grammar-used"
  ],
  "standard": {
    "globals": [
      "atom",
      "expect",
      "describe",
      "runs",
      "beforeEach",
      "it",
      "waitsForPromise",
      "it",
      "jasmine",
      "waitsForPromise"
    ]
  }
}
